iris.dist <- dist(x = iris[, -c(5)])
iris.clusters <- hclust(iris.dist, method="ward.D2")
plot(iris.clusters)
# Comparaison avec ACP
# On plonge notre tableau en 2D, donc on fait une ACP ou on prend que les
# deux première composantes principales
iris.scaled <- scale(iris[, -5], center=T, scale=T)
# Deux types de PCA possibles. Différence de résultat se situe sur les vecteurs propres
# qui ont été choisis pour les CP.
iris.pca <- princomp(iris.scaled)
iris.pca2 <- prcomp(iris.scaled)
biplot(iris.pca)
plot(iris.pca2$x[,1],iris.pca2$x[,2], col=cutree(iris.clusters, k=3))
# On peut voir que la classification en 3 semble marcher.
plot(iris.pca$scores[,1],iris.pca$scores[,2], col=cutree(iris.clusters, k=3))
